#ifndef LED_CONFIG_H
#define LED_CONFIG_H
/*Options : GPIOA 
            GPIOB
		  GBIOC */

#define LED_PORT  GPIOA

/*Options from PIN0 to PIN 7 */
#define LED_u8ID_1  PIN0
#define LED_u8ID_2  PIN1
#define LED_u8ID_3  PIN2
#define LED_u8ID_4  PIN3
#define LED_u8ID_5  PIN4
#define LED_u8ID_6  PIN5
#define LED_u8ID_7  PIN1
#define LED_u8ID_8  PIN7

/* options
 * OUTPUT MODE SPEED 10 MHZ
   OUTPUT_SPEED_10_MHZ_PP
   OUTPUT_SPEED_10_MHZ_OD
   OUTPUT_SPEED_10_MHZ_AF_PP
   OUTPUT_SPEED_10_MHZ_AF_OD

OUTPUT MODE SPEED 2MHZ
OUTPUT_SPEED_2_MHZ_PP     
OUTPUT_SPEED_2_MHZ_OD     
OUTPUT_SPEED_2_MHZ_AF_PP  
OUTPUT_SPEED_2_MHZ_AF_OD

OUTPUT MODE SPEED 50MHZ
OUTPUT_SPEED_50_MHZ_PP     
OUTPUT_SPEED_50_MHZ_OD     
