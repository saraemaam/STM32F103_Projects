#include "STD_TYPES.h"
#include"BIT_MATH.h"
#include"DIO_interface.h"
#include"RCC_interface.h"
#include"STK_interface.h"
#include"LEDMRX_interface.h"

/*	A		*/
//u8 DataArray [8]={0, 124, 18, 18, 18, 18, 124, 0};
u8 DataArray[8] ={1, 1, 1, 1, 1, 1, 1, 1};
void main(void){
	/*initialize clock system */
	RCC_voidInitSysClock();
	/*Enable GPIOA clock */
	RCC_voidEnableClock(RCC_APB2,2);
	/*Enable GPIOB*/
	RCC_voidEnableClock(RCC_APB2,3);
	/*Systick initialization*/
	MSTK_voidInit();
	/*initialize led matrix */
	HLEDMRX_voidInit();
   /* send data to led matrix */
	MGPIO_voidSetPinDir(GPIOA , PIN0 , OUTPUT_SPEED_2_MHZ_PP);
	MGPIO_voidSetPinDir(GPIOB , PIN9 , OUTPUT_SPEED_2_MHZ_PP);
	while(1)
	{
	//HLEDMRX_voidDisplay(DataArray);
	MGPIO_voidSetPinVal(GPIOA , PIN0 , HIGH); // A0 = 1 led
	MGPIO_voidSetPinVal(GPIOB , PIN9 , HIGH); // B0 = 0 GND
	}
}

